#include<stdio.h>
#include<math.h>

int main(){
int a[3][5];
    for(int i=0;i<3;i++){
        for(int j=0;j<5;j++){
            scanf("%d",&a[i][j]);
        }
    }
    for(int i=0;i<5;i++){
            int max=0;
        for(int j=0;j<3;j++){
                if(max<a[j][i])
                    max=a[j][i];
        }
        printf("max in each column is %d\n",max);
    }

return 0;
}
